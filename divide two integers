def divide(self, dividend, divisor):
        INT_MAX=2**31-1
        INT_MIN=-2**31
        if dividend == INT_MIN and divisor==-1:
            return INT_MAX
        negatives=2
        if dividend>0:
            negatives-=1
            dividend=-dividend
        if divisor>0:
            negatives-=1
            divisor=-divisor
        quotient=0
        while dividend<=divisor:
            temp_divisor=divisor
            num_divisors=1
            while dividend - temp_divisor<=temp_divisor:
                temp_divisor<<=1
                num_divisors<<=1
            dividend-=temp_divisor
            quotient+=num_divisors
        if negatives!=1:
            return min(max(quotient,INT_MIN),INT_MAX)
        else:
            return min(max(-quotient,INT_MIN),INT_MAX)
                
